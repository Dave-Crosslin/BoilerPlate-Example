    "dev": "^0.1.3",
    "express": "^4.16.3",
    "immutable": "^4.0.0-rc.9",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "tether": "^1.4.4"
  },
